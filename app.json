{
  "expo": {
    "name": "cubr",
    "description": "A very interesting project.",
    "slug": "cubr",
    "privacy": "public",
    "sdkVersion": "30.0.0",
    "platforms": ["ios", "android"],
    "ios": {
      "bundleIdentifier": "com.hackgt.cubr"
    },
    "android": {
      "package": "com.hackgt.cubr",
      "config": {
        "googleMaps": { "apiKey": "AIzaSyDbHUfclPFHMkvKrOowb-dcrf26u1gzrqI" }
      }
    },
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    }
  }
}
